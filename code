  pathsubsets = scalepath, # Location of files created by dBBMM.build. No terminal slash.
  pattern = ".asc",
  weighting = 1,
  # weighting to divide individual and summed-scaled rasters by, for unbalanced arrays
  format = "ascii",
  datatype = "FLT4S",
  bylayer = TRUE,
  overwrite = TRUE,
